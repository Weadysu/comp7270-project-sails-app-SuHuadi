<div class="container">
    <div class="row">
        <div class="card col-12 col-sm-12 col-lg-12 col-xl-12">
            <h1><%= rental.propertyTitle %></h1>
            <div class="row">
                <img class="img-fluid col-12 col-sm-8 col-lg-8 col-xl-8" src="<%= rental.imageURL %>" />
                <ul class="list-group col-12 col-sm-6 col-lg-4 col-xl-4">
                    <li class="list-group-item"><b>Estate: </b><%= rental.estate %></li>
                    <li class="list-group-item"><b>Gross Area: </b>$<%= rental.grossArea %></li>
                    <li class="list-group-item"><b>Rent: </b><%= rental.rent %></li>
                    <li class="list-group-item"><b>Bedrooms: </b><%= rental.bedrooms %></li>
                    <li class="list-group-item"><b>Expected Tenants: </b><%= rental.expectedTenants %></li>
                    <li class="list-group-item"><b>Created: </b><%= rental.date %></li>
                    <li class="list-group-item"><b>Updated: </b><%= rental.Rent %></li>
                </ul>
            </div>
        </div>
    </div>
</div>